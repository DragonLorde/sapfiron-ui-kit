{
  "name": "@dragonlorde/sapfiron-ui-kit",
  "version": "1.6.3",
  "description": "React components kit authored Sapfiron(Anokhin.D)",
  "homepage": "/",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "storybook": "NODE_PATH=src start-storybook -p 6006",
    "storybook:win": "set NODE_PATH=src && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eslint": "eslint --color --format html --output-file eslintreport.html src/**",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@dragonlorde"
  },
  "author": "dragonLorde",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.1.3",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/gitlab": "9.1.0",
    "@semantic-release/npm": "9.0.1",
    "@storybook/addon-actions": "6.5.4",
    "@storybook/addon-essentials": "6.5.4",
    "@storybook/addon-links": "6.5.4",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/preset-ant-design": "^0.0.2",
    "@storybook/react": "6.5.4",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "18.6.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-portal": "^4.0.4",
    "@types/styled-components": "5.1.25",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "conventional-changelog": "3.1.25",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "eslint": "^8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.0",
    "rollup": "2.67.2",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.31.2",
    "semantic-release": "19.0.2",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "5.5.0",
    "**/@typescript-eslint/parser": "5.5.0"
  },
  "dependencies": {
    "@types/d3": "^7.4.0",
    "@types/react-router-dom": "^5.3.3",
    "antd": "4.20.6",
    "antd-mask-input": "2.0.7",
    "classnames": "2.3.1",
    "csstype": "3.0.11",
    "d3": "^7.6.1",
    "lodash": "4.17.21",
    "moment-timezone": "0.5.34",
    "react-hook-form": "^7.34.2",
    "react-idle-timer": "5.2.0",
    "react-portal": "4.2.2",
    "react-router-dom": "^6.4.2",
    "styled-components": "5.3.5",
    "underscore": "1.13.2"
  },
  "repository": {
    "type": "git",
    "directory": "https://github.com/DragonLorde/sapfiron-ui-kit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/model/**",
      "!**/style/**",
      "!**/build/**",
      "!**/stories/**",
      "!**/types/**",
      "!**/components/**/index.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "!**/__tests__/**/*model*.[jt]s?(x)"
    ]
  }
}
